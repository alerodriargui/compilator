// Definición de terminales
terminal Integer NUM;
terminal PLUS, MINUS, TIMES, DIVIDE;

// Definición de no terminales
non terminal expr;

// Definición de precedencias
precedence left PLUS, MINUS;
precedence left TIMES, DIVIDE;

// Producciones
expr ::= expr PLUS expr
      | expr MINUS expr
      | expr TIMES expr
      | expr DIVIDE expr
      | NUM;
